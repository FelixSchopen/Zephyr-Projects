/ {
    
    gpio-pins {
        compatible = "gpio-keys";
        clk: pb6 {
            gpios = <&gpiob 6 GPIO_ACTIVE_HIGH>;
        };
        dt: pb7 {
            gpios = <&gpiob 7 GPIO_ACTIVE_HIGH>;
        };
        sw: pb8 {
            gpios = <&gpiob 8 GPIO_ACTIVE_HIGH>;
        };
    };

};